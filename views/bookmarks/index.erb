<h1>Bookmark list:</h1>

<ul>
  <% @bookmarks.each do |bookmark| %>
    <li><a href="<%=bookmark[:url]%>"><%=bookmark[:title]%></a>
</li>
  <% end %>
</ul>

<p1> Add Bookmark</p1>

<form action="/add" method="post">
  <input type="text" name="url">
  <input type="submit" value="submit">
  <input type="text" name="title">
  <input type="submit" value="submit">
</form>
